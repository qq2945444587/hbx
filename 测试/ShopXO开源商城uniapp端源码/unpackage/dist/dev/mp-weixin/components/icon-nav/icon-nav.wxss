
.icon-nav-list {
    overflow: hidden;
    margin-bottom: 20rpx;
}
.icon-nav-list .item {
    width: calc(20% - 20rpx);
    float: left;
    padding: 20rpx 10rpx 0 10rpx;
}
.icon-nav-list .item .item-content {
    border-radius: 50%;
    padding: 20rpx;
    text-align: center;
    width: 50rpx;
    height: 50rpx;
    margin: 0 auto;
    box-shadow: 0 2px 12px rgb(226 226 226 / 95%);
}
.icon-nav-list .item image {
    width: 50rpx !important;
    height: 50rpx !important;
}
.icon-nav-list .item .title {
    margin-top: 6rpx;
    font-size: 28rpx;
    text-align: center;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    max-width: 100%;
    color: #888;
}

