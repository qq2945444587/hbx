	opacity: 0;
	pointer-events: none;
	z-index: 100;
}
.yzm_zz.data-v-b237504c{
	position: fixed;
	background: #fff;
	z-index: 101;
	bottom: 0px;
	width: 100%;
	overflow: hidden;
	left: 0;
	transition: all 0.25s linear;
	border-top-right-radius: 20rpx;
	border-top-left-radius: 20rpx;
	-webkit-transform: translateY(100%);
	transform: translateY(100%);
}
.active .zhezhao.data-v-b237504c{
	opacity: 1;
	pointer-events: auto;
}
